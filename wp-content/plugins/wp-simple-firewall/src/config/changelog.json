	  "version": "5.11.1"
	},
	{
	  "type": "added",
	  "summary": "Better Shield Central page.",
	  "version": "5.11.0"
	},
	{
	  "type": "changed",
	  "summary": "Removed unused options sections.",
	  "version": "5.11.0"
	},
	{
	  "type": "fixed",
