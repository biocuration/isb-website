  "changelog": [
	{
	  "type": "added",
	  "summary": "Option for administrators to manually override and set the source of the visitor IP address.",
	  "version": "5.14.0"
	},
	{
	  "type": "updated",
	  "summary": "In-plugin documentation links to updated and revised helpdesk articles/blogs.",
	  "version": "5.14.0"
	},
	{
	  "type": "improved",
	  "summary": "Strip out any non-alphanumeric characters uses in the generation of Google Authenticator URLs.",
	  "version": "5.14.0"
	},
	{
	  "type": "fixed",
	  "summary": "Shield now ignores any requests sent to Rest API URIs with respect to Shield user sessions.",
	  "version": "5.14.0"
	},
	{
	  "type": "improved",
	  "summary": "Small adjustment to handling of Shield User sessions in conjunction with WordPress sessions.",
	  "version": "5.13.2"
	},
	{
	  "type": "fixed",
	  "summary": "Restore display of help links for options.",
	  "version": "5.13.2"
	},
	{
	  "type": "fixed",
	  "summary": "PHP 5.2 incompatibility with anonymous function.",
	  "version": "5.13.1"
	},
	{
	  "type": "added",
	  "summary": "New option for Unrecognised File Scanner to scan the Uploads folder for JS and PHP files.",
	  "version": "5.13.0"
	},
	{
	  "type": "added",
	  "summary": "Option to provide custom list of files to be excluded from the Unrecognised File Scanner.",
	  "version": "5.13.0"
	},
	{
	  "type": "improved",
	  "summary": "Improved support for Windows IIS hosting for Unrecognised File Scanner.",
	  "version": "5.12.2"
	},
	{
	  "type": "changed",
	  "summary": "Removed the email-based 2FA automatic login link.",
	  "version": "5.12.2"
	},
	{
	  "type": "fixed",
	  "summary": "Potential bug with Shield not recognising plugin configuration updates and not rebuilding options accordingly.",
	  "version": "5.12.2"
	},
	{
	  "type": "added",
	  "summary": "A few more exclusions for the Unrecognised File Scanner.",
	  "version": "5.12.1"
	},
	{
	  "type": "fixed",
	  "summary": "Fix for Fatal error: Class ‘ICWP_WPSF_FeatureHandler_HackProtect’ not found.",
	  "version": "5.12.1"
	},
	{
	  "type": "added",
	  "summary": "Unrecognised File Scanner.",
	  "version": "5.12.0"
	},
	{
	  "type": "fixed",
	  "summary": "JSON syntax error.",
	  "version": "5.11.1"
	},
	{
	  "type": "added",
	  "summary": "Better Shield Central page.",
	  "version": "5.11.0"
	},
	{
	  "type": "changed",
	  "summary": "Removed unused options sections.",
	  "version": "5.11.0"
	},
	{
	  "type": "fixed",
	  "summary": "Fatal error with GASP + Password Reset.",
	  "version": "5.10.2"
	},
	{
	  "type": "fixed",
	  "summary": "Fatal error with failing reCAPTCHA HTTP requests.",
	  "version": "5.10.2"
	},
	{
	  "type": "improved",
	  "summary": "Further preparation for the Shield Central release.",
	  "version": "5.10.1"
	},
	{
	  "type": "added",
	  "summary": "More in-depth reporting and statistics gathering - options for reports will be made available in a later release.",
	  "version": "5.10.0"
	},
	{
	  "type": "added",
	  "summary": "Help Videos for 1 or 2 modules. More to come and just testing format and uptake.",
	  "version": "5.9.0"
	},
    {
      "type": "added",
      "summary": "Special handling for WP Fastest Cache.",
      "version": "5.9.0"
    },
    {
      "type": "changed",
      "summary": "Automatic Self-Update no longer configurable. If automatic updates module is disabled, will update within 1 week.",
      "version": "5.9.0"
    },
    {
      "type": "changed",
      "summary": "No longer remove an existing user session when accessed from another IP address. Just redirect.",
      "version": "5.9.0"
    },
    {
      "type": "fixed",
      "summary": "Danish string translation.",
      "version": "5.9.0"
    },
    {
      "type": "improved",
      "summary": "The core file scanner now works more reliably for international WordPress installations.",
      "version": "5.8.2"
    },
    {
      "type": "changed",
      "summary": "Login Cooldown now uses only the flag file as an indicator of login times.",
      "version": "5.8.2"
    },
    {
      "type": "added",
      "summary": "Filter to allow for changing the login two factor timeout period, from 5 (minutes). Filter: icwp-wpsf-login_intent_timeout",
      "version": "5.8.2"
    },
    {
      "type": "changed",
      "summary": "Changed timeout for two-factor authentication email to 5 minutes to account for slower email sending providers.",
      "version": "5.8.2"
    },
    {
      "type": "changed",
      "summary": "Added further clarification to the Login Notification email indicating that two-factor authentication was pending.",
      "version": "5.8.2"
    },
    {
      "type": "fixed",
      "summary": "Fixed a couple of bugs with the Login Authentication Portal, for certain edge cases.",
      "version": "5.8.1"
    },
    {
      "type": "changed",
      "summary": "Major overhaul of Two-Factor / Multi-Factor Login Authentication",
      "link": "http://icwp.io/87",
      "version": "5.8.0"
    },
    {
      "type": "changed",
      "summary": "Introduction of Login Authentication Portal for improved Multi-Factor Authentication",
      "link": "http://icwp.io/86",
      "version": "5.8.0"
    },
    {
      "type": "added",
      "summary": "Option to choose between two-factor or multi-factor login authentication",
      "version": "5.8.0"
    },
    {
      "type": "added",
      "summary": "Administrators can remove Google Authenticator from another user’s profile",
      "version": "5.8.0"
    },
    {
      "type": "added",
      "summary": "When Security Admin is active, only Security Admins may remove Google Authenticator from other admins",
      "version": "5.8.0"
    },
    {
      "type": "changed",
      "summary": "Yubikey login authentication is now managed directly from the User Profile screen, as with Google Authenticator",
      "version": "5.8.0"
    },
    {
      "type": "changed",
      "summary": "Email-based login authentication no longer uses a separate database table",
      "version": "5.8.0"
    },
    {
      "type": "fixed",
      "summary": "Core file scanning now adequately handles Windows/Unix new lines during scan",
      "version": "5.8.0"
    },
    {
      "type": "fixed",
      "summary": "Certain crons weren’t setup correctly",
      "version": "5.8.0"
    },
    {
      "type": "improvements",
      "summary": "Further preparation for Shield Central release",
      "link": "http://icwp.io/83",
      "version": "5.8.0"
    }
  ]
}